{
  "env": {
    "production": {
      "plugins": [["emotion", { "hoist": true }]],
      "presets": ["minify"]
    },
    "development": {
      "plugins": [["emotion", { "sourceMap": true, "autoLabel": true }]]
    },
    "test": {
      "presets": ["env", "react"]
    }
  },
  "presets": [["env", { "modules": false }], "react"],
  "plugins": ["emotion", "react-hot-loader/babel"]
}
